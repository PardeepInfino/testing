    <body>
        <referenceContainer name="page.content">
            <block before="page_main_actions" class="Xtento\StockImport\Block\Adminhtml\Widget\Menu" name="stockimport.menu"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Xtento\StockImport\Block\Adminhtml\Source" name="adminhtml.stockimport.source.grid.container"/>
        </referenceContainer>
    </body>
</page>

