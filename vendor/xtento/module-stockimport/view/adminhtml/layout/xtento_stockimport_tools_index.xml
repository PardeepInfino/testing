<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="page.content">
            <block before="page_main_actions" class="Xtento\StockImport\Block\Adminhtml\Widget\Menu" name="stockimport.menu"/>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Xtento\StockImport\Block\Adminhtml\Tools" name="adminhtml.stockimport.tools">
                <block class="Xtento\StockImport\Block\Adminhtml\Tools\Import" name="adminhtml.stockimport.tools.import" template="Xtento_StockImport::tools/import.phtml"/>
                <block class="Xtento\StockImport\Block\Adminhtml\Tools\Export" name="adminhtml.stockimport.tools.export" template="Xtento_StockImport::tools/export.phtml"/>
            </block>
        </referenceContainer>
    </body>
</page>

